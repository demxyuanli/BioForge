    "core:window:allow-start-dragging",
    "core:window:allow-is-maximized",
    "dialog:default",
    "fs:default",
    "fs:scope-app",
    "fs:scope-appcache",
    "fs:scope-appconfig",
    "fs:scope-appdata",
    "fs:scope-applocaldata",
    "fs:scope-applog",
