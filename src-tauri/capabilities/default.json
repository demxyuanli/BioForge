    "fs:scope-config",
    "fs:scope-data",
    "fs:scope-desktop",
    "fs:scope-desktop-recursive",
    "fs:scope-document",
    "fs:scope-document-recursive",
    "fs:scope-download",
    "fs:scope-download-recursive",
    "fs:scope-exe",
    "fs:scope-font",
    "fs:scope-home",
